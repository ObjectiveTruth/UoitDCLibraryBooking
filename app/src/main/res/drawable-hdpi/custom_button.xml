<?xml version="1.0" encoding="utf-8"?>
<selector
xmlns:android="http://schemas.android.com/apk/res/android">
<item         
    
    android:state_enabled="false"> 
	
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
 
   <item 
       
       >
       <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
		<corners
		android:radius="20dp"
		/>
		<solid
		android:color="#C8DBDB"
		/>
		<padding
		android:left="0dp"
		android:top="0dp"
		android:right="0dp"
		android:bottom="0dp"
		/>
		<size
		android:width="130dp"
		android:height="40dp"
		/>


		</shape>
		

		       
     </item>
 
</layer-list>
</item>
<item         
    android:state_selected="true"
    android:state_enabled="true"> 
	
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
 
   <item 
       >
       <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
		<corners
		android:radius="20dp"
		/>
		<solid
		android:color="#849191"
		/>
		<padding
		android:left="0dp"
		android:top="0dp"
		android:right="0dp"
		android:bottom="0dp"
		/>
		<size
		android:width="130dp"
		android:height="40dp"
		/>


		</shape>
		

		       
     </item>
 
</layer-list>
</item>
<item    
    android:state_pressed="true"
    android:state_enabled="true"
    
    
        >

      <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
		<corners
		android:radius="20dp"
		/>
		<solid
		android:color="#849191"
		/>
		<padding
		android:left="0dp"
		android:top="0dp"
		android:right="0dp"
		android:bottom="0dp"
		/>
		<size
		android:width="130dp"
		android:height="40dp"
		/>
		<stroke
		android:width="5dp"
		android:color="@null"
		/>


	</shape>

 

</item>

<item>

      <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
		<corners
		android:radius="20dp"
		/>
		<solid
		android:color="#5F6969"
		/>
		<padding
		android:left="0dp"
		android:top="0dp"
		android:right="0dp"
		android:bottom="0dp"
		/>
		<size
		android:width="130dp"
		android:height="40dp"
		/>
	</shape>

 

</item>
</selector>